{
  "name": "yui-configger",
  "version": "0.2.2",
  "description": "Extract meta-data from a folder of YUI modules & generate a Loader config",
  "preferGlobal": true,
  "author": "Pat Cavit <pcavit@gmail.com",
  "bugs": "https://github.com/tivac/yui-configger/issues",
  "bin": "./bin/cli.js",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tivac/yui-configger.git"
  },
  "dependencies": {
    "esprima": "1.0.x",
    "escodegen": "~1.0.1",
    "estraverse": "~1.5.0",
    "wrench": "1.5.x",
    "optimist": "0.6.x",
    "clone": "0.1.x",
    "npmlog": "0.0.x",
    "escape-regexp-component": "1.0.x"
  },
  "devDependencies": {
    "mocha": "~1.16.2",
    "istanbul": "~0.2.3"
  },
  "scripts": {
    "test": "istanbul test --print both ./node_modules/mocha/bin/_mocha",
    "debug": "nodebug ./node_modules/mocha/bin/_mocha",
    "watch": "supervisor -n \"error|exit\" ./node_modules/mocha/bin/_mocha"
  },
  "license": "MIT",
  "engine": {
    "node": ">=0.8"
  },
  "keywords": [
    "yui",
    "config",
    "esprima"
  ]
}
